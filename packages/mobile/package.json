{
  "name": "@audiio/mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile web portal for Audiio - access your music anywhere",
  "main": "./dist/server/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:web": "vite",
    "build": "npm run build:server && npm run build:web",
    "build:server": "tsc -p tsconfig.server.json",
    "build:web": "vite build",
    "start": "node dist/server/index.js",
    "preview": "vite preview",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@audiio/core": "*",
    "@audiio/icons": "*",
    "@audiio/relay": "*",
    "@fastify/cors": "^10.0.1",
    "@fastify/rate-limit": "^10.0.1",
    "@fastify/static": "^8.0.0",
    "@fastify/websocket": "^11.0.1",
    "electron": "39.2.7",
    "fastify": "^5.1.0",
    "nanoid": "^3.3.11",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "ws": "^8.18.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^7.3.0"
  }
}
