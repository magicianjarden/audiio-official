{
  "name": "@audiio/ml-sdk",
  "version": "1.0.0",
  "description": "SDK for building Audiio ML algorithm plugins",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./types": {
      "types": "./dist/esm/types/index.d.ts",
      "import": "./dist/esm/types/index.js",
      "require": "./dist/cjs/types/index.js"
    },
    "./base": {
      "types": "./dist/esm/base/index.d.ts",
      "import": "./dist/esm/base/index.js",
      "require": "./dist/cjs/base/index.js"
    },
    "./utils": {
      "types": "./dist/esm/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    },
    "./testing": {
      "types": "./dist/esm/testing/index.d.ts",
      "import": "./dist/esm/testing/index.js",
      "require": "./dist/cjs/testing/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs-package": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^4.0.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "audiio",
    "ml",
    "sdk",
    "music",
    "recommendation",
    "algorithm"
  ],
  "author": "Audiio",
  "license": "MIT"
}
