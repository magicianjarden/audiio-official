{
  "name": "@audiio/server",
  "version": "0.1.0",
  "description": "Audiio Standalone Server - Run your music server anywhere",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "audiio-server": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc",
    "start": "node dist/cli.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@audiio/core": "workspace:*",
    "@tensorflow/tfjs": "^4.22.0",
    "essentia.js": "^0.1.3",
    "@fastify/cors": "^10.0.1",
    "bonjour-service": "^1.3.0",
    "@fastify/rate-limit": "^10.0.1",
    "@fastify/static": "^8.0.0",
    "@fastify/websocket": "^11.0.1",
    "better-sqlite3": "^11.6.0",
    "chokidar": "^3.6.0",
    "fastify": "^5.1.0",
    "music-metadata": "^7.14.0",
    "nanoid": "^3.3.11",
    "qrcode": "^1.5.3",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "ws": "^8.18.0",
    "yaml": "^2.3.4",
    "youtubei.js": "^16.0.1",
    "node-id3": "^0.2.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/ws": "^8.5.13",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
